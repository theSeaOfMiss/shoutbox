<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
</head>
<body>
    <% include menu %>



    <% entries.forEach(function (entry) { %>
        <div class="entry">
            <h3><%= entry.title %></h3>
            <p><%= entry.body %></p>
            <p>Post by <%= entry.user.username %></p>
        </div>
    <% }) %>

    <% include pager.ejs %>
</body>
</html>